[user]
	email = jas.kang@gmail.com
	name = Jason Kang
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[include]
    path = ./themes.config
[delta]
    features = arctic-fox
    navigate = true
    side-by-side = true
[merge]
    conflictStyle = diff3
[diff]
    colorMoved = default
[push]
	autoSetupRemote = true
[init]
	defaultBranch = main
